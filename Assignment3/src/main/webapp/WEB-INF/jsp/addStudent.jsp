	<script src="/js/sendFormData.js"></script>
	
</head>
	<body>
		<form name = "myForm" id="myForm">
			<center>
				<div class="popup">
					<span class="poputext" id="invalidNamePopup" style="color: red;"></span>
				</div>
				<div class="popup">
					<span class="popuptext" id="invalidEmailPopup" style="color: red;"></span>
				</div>
				<table width = "30%" cellpadding = "7" frame="box" rules="none">
					<thead>
						<tr>
							<th colspan = "2"> Enter information here</th>
						</tr>
					</thead>
					<tbody>
						<tr>
							<td>Name</td>
							<td><input type="text" name="name" id="name" value="" /></td>
						</tr>
						<tr>
							<td>Email</td>
							<td><input type="text" name="email" id="email" value="" /></td>
						</tr>
						<tr>
							<td>Group</td>
							<td><input type="text" name="group" id="group" value="" /></td>
						</tr>
						<tr>
							<td>Hobby</td>
							<td><input type="text" name="hobby" id="hobby" value="" /></td>
						</tr>
						<tr>
	                        <td><input type="button"  class="submitResetBtn" value="Submit" id="submitButton"/></td>
	                        <td><input type="reset" class="submitResetBtn" value="Reset" /></td>
	                    </tr>
					</tbody>
				</table>
			</center>
		</form>
	</body>
	
	<script>
	sendFormData("#submitButton", "student", "#myForm");
	
	function setOnChangeValidationMethod(textInputId, regExpString, popupId, errorMessage, buttonId) {
		jQuery("#" + textInputId).keyup(function() {
			var pattern = new RegExp(regExpString);
			if(pattern.test($(this).val())) {
				$("#" + popupId).text("");
				$("#" + buttonId).removeAttr('disabled');
			} else {
				$("#" + popupId).text(errorMessage);
				$("#" + buttonId).attr('disabled', 'disabled');
			}
		});
	}
	
